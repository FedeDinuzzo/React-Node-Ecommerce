<h1>Carrito {{cartID}}</h1>

<div class="prueba">
{{#each auxProducts}}
    <div class="cartProducts">
      <div class="prueba">
        <h2>{{title}}</h2>
      </div>
      <div>
        <p>{{description}}</p>      </div>
      <div>
        <p>Precio: ${{price}}</p>
      </div>
      <div>
        <p>Cantidad: {{quantity}}</p>
      </div>
        
    </div>
{{/each}}
</div>